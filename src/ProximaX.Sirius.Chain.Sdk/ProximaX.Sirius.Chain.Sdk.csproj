<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Version>0.4.0</Version>
    <Authors>Proximax IO</Authors>
    <Description>ProximaX Sirius Blockchain .Net SDK</Description>
    <Copyright>Copyright (c) 2019 ProximaX Limited</Copyright>
    <PackageProjectUrl>https://github.com/proximax-storage/csharp-xpx-catapult-sdk</PackageProjectUrl>
    <RepositoryUrl>https://github.com/proximax-storage/csharp-xpx-catapult-sdk</RepositoryUrl>
    <RepositoryType />
    <PackageReleaseNotes>- Fixed multisig account
- Fixed receipt</PackageReleaseNotes>
    <AssemblyVersion>0.0.4.0</AssemblyVersion>
    <FileVersion>0.0.4.0</FileVersion>
    <PackageId>csharp-xpx-chain-sdk</PackageId>
    <AssemblyName>csharp-xpx-chain-sdk</AssemblyName>
    <RootNamespace>ProximaX.Sirius.Chain.Sdk</RootNamespace>
    <LangVersion>latest</LangVersion>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <Optimize>false</Optimize>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Dap.FlatBuffers" Version="1.0.0" />
    <PackageReference Include="FluentValidation" Version="8.4.0" />
    <PackageReference Include="Flurl.Http" Version="2.4.2" />
    <PackageReference Include="Guard.Net" Version="1.2.0" />
    <PackageReference Include="Portable.BouncyCastle" Version="1.8.5" />
    <PackageReference Include="System.Reactive" Version="4.1.5" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
